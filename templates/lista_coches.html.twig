<!doctype html>

<html>
<meta charset="utf-8">
<body>

    <h1>Listado de coches</h1>
    {% if coches %}
        {% for coche in coches %}
        <ul>
            <li><strong>Marca: </strong>{{ coche.marca}}</li>
            <li><strong>Modelo: </strong>{{ coche.modelo}}</li>
            <li><strong>Año: </strong>{{ coche.año}}</li>
            <li><strong>Precio: </strong>{{ coche.precio}}</li>
        </ul>
        {% endfor %}
    {% else %}
        <p>No se han encontrado coches</p>
    {% endif %}
</body>

</html>